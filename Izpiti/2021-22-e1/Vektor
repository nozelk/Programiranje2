public class Vector {
    private double[] coordinates;

    public Vector() {
        this.coordinates = new double[3];
    }

    public Vector(int n) {
        this.coordinates = new double[n];
        Random random = new Random();
        for (int i = 0; i < n; i++) {
            this.coordinates[i] = random.nextDouble();
        }
    }

    public Vector(double[] coordinates) {
        this.coordinates = coordinates;
    }

    public static void main(String[] args) {
        Vector v1 = new Vector();
        Vector v2 = new Vector(5);
        Vector v3 = new Vector(new double[]{1.2, 3.4, 5.6, 7.8});

        System.out.println(Arrays.toString(v1.coordinates));
        System.out.println(Arrays.toString(v2.coordinates));
        System.out.println(Arrays.toString(v3.coordinates));
    }
}